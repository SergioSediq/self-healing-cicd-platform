GOOGLE_API_KEY=your_gemini_api_key_here
GITHUB_TOKEN=your_github_token_here

# Optional: Provider-specific (see INTEGRATIONS.md)
# JENKINS_URL= JENKINS_USER= JENKINS_TOKEN=
# AWS_REGION=us-east-1
# AZURE_DEVOPS_ORG= AZURE_DEVOPS_PROJECT= AZURE_DEVOPS_PAT=

# Optional: Agent tuning
# LLM_PRIMARY_MODEL=gemini-2.0-flash
# LLM_FALLBACK_MODEL=gemini-1.5-flash
# LOG_MAX_CHARS=16000
# CONFIDENCE_THRESHOLD=0.8

# Optional: Dashboard API auth (Bearer token)
# DASHBOARD_API_KEY= or DASHBOARD_VIEWER_KEY= / DASHBOARD_APPROVER_KEY=

# Optional: Alerting (Slack, PagerDuty)
# SLACK_WEBHOOK_URL= PAGERDUTY_ROUTING_KEY=
# ALERT_FAILURE_THRESHOLD=3

# Optional: Token budget
# TOKEN_BUDGET_ALERT=100000

# Feature flags
# FEATURE_QUORUM=1 FEATURE_STREAMING=1 FEATURE_RECOVERY=1
# FEATURE_JIRA_ON_LOW_CONFIDENCE=1

# Rate limiting (requests per minute per IP)
# RATE_LIMIT_PER_MINUTE=60
# RATE_LIMIT_DISABLED=1

# Integrations
# SLACK_WEBHOOK_URL= TEAMS_WEBHOOK_URL= DISCORD_WEBHOOK_URL=
# JIRA_URL= JIRA_EMAIL= JIRA_API_TOKEN= JIRA_PROJECT=
# GITLAB_TOKEN= GITLAB_URL=https://gitlab.com
# SERVICENOW_URL= SERVICENOW_USER= SERVICENOW_PASSWORD=
# WEBHOOK_URL= WEBHOOK_SECRET=

# Performance (Redis for queue + cache)
# REDIS_URL=redis://localhost:6379
# QUEUE_DIR=logs/queue  (file-based fallback)

# Observability
# CHECK_GITHUB=1  (health check GitHub connectivity)